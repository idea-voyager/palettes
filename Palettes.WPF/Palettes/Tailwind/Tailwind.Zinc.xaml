<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Color x:Key="Color-Zinc-050">#fafafa</Color>
    <Color x:Key="Color-Zinc-100">#f4f4f5</Color>
    <Color x:Key="Color-Zinc-200">#e4e4e7</Color>
    <Color x:Key="Color-Zinc-300">#d4d4d8</Color>
    <Color x:Key="Color-Zinc-400">#a1a1aa</Color>
    <Color x:Key="Color-Zinc-500">#71717a</Color>
    <Color x:Key="Color-Zinc-600">#52525b</Color>
    <Color x:Key="Color-Zinc-700">#3f3f46</Color>
    <Color x:Key="Color-Zinc-800">#27272a</Color>
    <Color x:Key="Color-Zinc-900">#18181b</Color>
    <Color x:Key="Color-Zinc-950">#09090b</Color>
    
    <SolidColorBrush x:Key="Brush-Zinc-050" Color="{StaticResource Color-Zinc-050}"/>
    <SolidColorBrush x:Key="Brush-Zinc-100" Color="{StaticResource Color-Zinc-100}"/>
    <SolidColorBrush x:Key="Brush-Zinc-200" Color="{StaticResource Color-Zinc-200}"/>
    <SolidColorBrush x:Key="Brush-Zinc-300" Color="{StaticResource Color-Zinc-300}"/>
    <SolidColorBrush x:Key="Brush-Zinc-400" Color="{StaticResource Color-Zinc-400}"/>
    <SolidColorBrush x:Key="Brush-Zinc-500" Color="{StaticResource Color-Zinc-500}"/>
    <SolidColorBrush x:Key="Brush-Zinc-600" Color="{StaticResource Color-Zinc-600}"/>
    <SolidColorBrush x:Key="Brush-Zinc-700" Color="{StaticResource Color-Zinc-700}"/>
    <SolidColorBrush x:Key="Brush-Zinc-800" Color="{StaticResource Color-Zinc-800}"/>
    <SolidColorBrush x:Key="Brush-Zinc-900" Color="{StaticResource Color-Zinc-900}"/>
    <SolidColorBrush x:Key="Brush-Zinc-950" Color="{StaticResource Color-Zinc-950}"/>
    
</ResourceDictionary>