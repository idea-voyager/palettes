<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Color x:Key="Color-Blue-050">#eff6ff</Color>
    <Color x:Key="Color-Blue-100">#dbeafe</Color>
    <Color x:Key="Color-Blue-200">#bfdbfe</Color>
    <Color x:Key="Color-Blue-300">#93c5fd</Color>
    <Color x:Key="Color-Blue-400">#60a5fa</Color>
    <Color x:Key="Color-Blue-500">#3b82f6</Color>
    <Color x:Key="Color-Blue-600">#2563eb</Color>
    <Color x:Key="Color-Blue-700">#1d4ed8</Color>
    <Color x:Key="Color-Blue-800">#1e40af</Color>
    <Color x:Key="Color-Blue-900">#1e3a8a</Color>
    <Color x:Key="Color-Blue-950">#172554</Color>
    
    <SolidColorBrush x:Key="Brush-Blue-050" Color="{StaticResource Color-Blue-050}"/>
    <SolidColorBrush x:Key="Brush-Blue-100" Color="{StaticResource Color-Blue-100}"/>
    <SolidColorBrush x:Key="Brush-Blue-200" Color="{StaticResource Color-Blue-200}"/>
    <SolidColorBrush x:Key="Brush-Blue-300" Color="{StaticResource Color-Blue-300}"/>
    <SolidColorBrush x:Key="Brush-Blue-400" Color="{StaticResource Color-Blue-400}"/>
    <SolidColorBrush x:Key="Brush-Blue-500" Color="{StaticResource Color-Blue-500}"/>
    <SolidColorBrush x:Key="Brush-Blue-600" Color="{StaticResource Color-Blue-600}"/>
    <SolidColorBrush x:Key="Brush-Blue-700" Color="{StaticResource Color-Blue-700}"/>
    <SolidColorBrush x:Key="Brush-Blue-800" Color="{StaticResource Color-Blue-800}"/>
    <SolidColorBrush x:Key="Brush-Blue-900" Color="{StaticResource Color-Blue-900}"/>
    <SolidColorBrush x:Key="Brush-Blue-950" Color="{StaticResource Color-Blue-950}"/>
    
</ResourceDictionary>