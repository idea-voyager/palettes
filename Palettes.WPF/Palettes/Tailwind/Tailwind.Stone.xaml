<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Color x:Key="Color-Stone-050">#fafaf9</Color>
    <Color x:Key="Color-Stone-100">#f5f5f4</Color>
    <Color x:Key="Color-Stone-200">#e7e5e4</Color>
    <Color x:Key="Color-Stone-300">#d6d3d1</Color>
    <Color x:Key="Color-Stone-400">#a8a29e</Color>
    <Color x:Key="Color-Stone-500">#78716c</Color>
    <Color x:Key="Color-Stone-600">#57534e</Color>
    <Color x:Key="Color-Stone-700">#44403c</Color>
    <Color x:Key="Color-Stone-800">#292524</Color>
    <Color x:Key="Color-Stone-900">#1c1917</Color>
    <Color x:Key="Color-Stone-950">#0f0e0e</Color>
    
    <SolidColorBrush x:Key="Brush-Stone-050" Color="{StaticResource Color-Stone-050}"/>
    <SolidColorBrush x:Key="Brush-Stone-100" Color="{StaticResource Color-Stone-100}"/>
    <SolidColorBrush x:Key="Brush-Stone-200" Color="{StaticResource Color-Stone-200}"/>
    <SolidColorBrush x:Key="Brush-Stone-300" Color="{StaticResource Color-Stone-300}"/>
    <SolidColorBrush x:Key="Brush-Stone-400" Color="{StaticResource Color-Stone-400}"/>
    <SolidColorBrush x:Key="Brush-Stone-500" Color="{StaticResource Color-Stone-500}"/>
    <SolidColorBrush x:Key="Brush-Stone-600" Color="{StaticResource Color-Stone-600}"/>
    <SolidColorBrush x:Key="Brush-Stone-700" Color="{StaticResource Color-Stone-700}"/>
    <SolidColorBrush x:Key="Brush-Stone-800" Color="{StaticResource Color-Stone-800}"/>
    <SolidColorBrush x:Key="Brush-Stone-900" Color="{StaticResource Color-Stone-900}"/>
    <SolidColorBrush x:Key="Brush-Stone-950" Color="{StaticResource Color-Stone-950}"/>
    
</ResourceDictionary>