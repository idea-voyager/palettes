<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Color x:Key="Color-Orange-050">#fff7ed</Color>
    <Color x:Key="Color-Orange-100">#ffedd5</Color>
    <Color x:Key="Color-Orange-200">#fed7aa</Color>
    <Color x:Key="Color-Orange-300">#fdba74</Color>
    <Color x:Key="Color-Orange-400">#fb923c</Color>
    <Color x:Key="Color-Orange-500">#f97316</Color>
    <Color x:Key="Color-Orange-600">#ea580c</Color>
    <Color x:Key="Color-Orange-700">#c2410c</Color>
    <Color x:Key="Color-Orange-800">#9a3412</Color>
    <Color x:Key="Color-Orange-900">#7c2d12</Color>
    <Color x:Key="Color-Orange-950">#431407</Color>
    
    <SolidColorBrush x:Key="Brush-Orange-050" Color="{StaticResource Color-Orange-050}"/>
    <SolidColorBrush x:Key="Brush-Orange-100" Color="{StaticResource Color-Orange-100}"/>
    <SolidColorBrush x:Key="Brush-Orange-200" Color="{StaticResource Color-Orange-200}"/>
    <SolidColorBrush x:Key="Brush-Orange-300" Color="{StaticResource Color-Orange-300}"/>
    <SolidColorBrush x:Key="Brush-Orange-400" Color="{StaticResource Color-Orange-400}"/>
    <SolidColorBrush x:Key="Brush-Orange-500" Color="{StaticResource Color-Orange-500}"/>
    <SolidColorBrush x:Key="Brush-Orange-600" Color="{StaticResource Color-Orange-600}"/>
    <SolidColorBrush x:Key="Brush-Orange-700" Color="{StaticResource Color-Orange-700}"/>
    <SolidColorBrush x:Key="Brush-Orange-800" Color="{StaticResource Color-Orange-800}"/>
    <SolidColorBrush x:Key="Brush-Orange-900" Color="{StaticResource Color-Orange-900}"/>
    <SolidColorBrush x:Key="Brush-Orange-950" Color="{StaticResource Color-Orange-950}"/>
    
</ResourceDictionary>