<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Color x:Key="Color-Purple-050">#faf5ff</Color>
    <Color x:Key="Color-Purple-100">#f3e8ff</Color>
    <Color x:Key="Color-Purple-200">#e9d5ff</Color>
    <Color x:Key="Color-Purple-300">#d8b4fe</Color>
    <Color x:Key="Color-Purple-400">#c084fc</Color>
    <Color x:Key="Color-Purple-500">#a855f7</Color>
    <Color x:Key="Color-Purple-600">#9333ea</Color>
    <Color x:Key="Color-Purple-700">#7e22ce</Color>
    <Color x:Key="Color-Purple-800">#6b21a8</Color>
    <Color x:Key="Color-Purple-900">#581c87</Color>
    <Color x:Key="Color-Purple-950">#3b0764</Color>
    
    <SolidColorBrush x:Key="Brush-Purple-050" Color="{StaticResource Color-Purple-050}"/>
    <SolidColorBrush x:Key="Brush-Purple-100" Color="{StaticResource Color-Purple-100}"/>
    <SolidColorBrush x:Key="Brush-Purple-200" Color="{StaticResource Color-Purple-200}"/>
    <SolidColorBrush x:Key="Brush-Purple-300" Color="{StaticResource Color-Purple-300}"/>
    <SolidColorBrush x:Key="Brush-Purple-400" Color="{StaticResource Color-Purple-400}"/>
    <SolidColorBrush x:Key="Brush-Purple-500" Color="{StaticResource Color-Purple-500}"/>
    <SolidColorBrush x:Key="Brush-Purple-600" Color="{StaticResource Color-Purple-600}"/>
    <SolidColorBrush x:Key="Brush-Purple-700" Color="{StaticResource Color-Purple-700}"/>
    <SolidColorBrush x:Key="Brush-Purple-800" Color="{StaticResource Color-Purple-800}"/>
    <SolidColorBrush x:Key="Brush-Purple-900" Color="{StaticResource Color-Purple-900}"/>
    <SolidColorBrush x:Key="Brush-Purple-950" Color="{StaticResource Color-Purple-950}"/>
    
</ResourceDictionary>