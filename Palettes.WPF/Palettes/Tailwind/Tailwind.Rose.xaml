<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Color x:Key="Color-Rose-050">#fff1f2</Color>
    <Color x:Key="Color-Rose-100">#ffe4e6</Color>
    <Color x:Key="Color-Rose-200">#fecdd3</Color>
    <Color x:Key="Color-Rose-300">#fda4af</Color>
    <Color x:Key="Color-Rose-400">#fb7185</Color>
    <Color x:Key="Color-Rose-500">#f43f5e</Color>
    <Color x:Key="Color-Rose-600">#e11d48</Color>
    <Color x:Key="Color-Rose-700">#be123c</Color>
    <Color x:Key="Color-Rose-800">#9f1239</Color>
    <Color x:Key="Color-Rose-900">#881337</Color>
    <Color x:Key="Color-Rose-950">#4c0519</Color>
    
    <SolidColorBrush x:Key="Brush-Rose-050" Color="{StaticResource Color-Rose-050}"/>
    <SolidColorBrush x:Key="Brush-Rose-100" Color="{StaticResource Color-Rose-100}"/>
    <SolidColorBrush x:Key="Brush-Rose-200" Color="{StaticResource Color-Rose-200}"/>
    <SolidColorBrush x:Key="Brush-Rose-300" Color="{StaticResource Color-Rose-300}"/>
    <SolidColorBrush x:Key="Brush-Rose-400" Color="{StaticResource Color-Rose-400}"/>
    <SolidColorBrush x:Key="Brush-Rose-500" Color="{StaticResource Color-Rose-500}"/>
    <SolidColorBrush x:Key="Brush-Rose-600" Color="{StaticResource Color-Rose-600}"/>
    <SolidColorBrush x:Key="Brush-Rose-700" Color="{StaticResource Color-Rose-700}"/>
    <SolidColorBrush x:Key="Brush-Rose-800" Color="{StaticResource Color-Rose-800}"/>
    <SolidColorBrush x:Key="Brush-Rose-900" Color="{StaticResource Color-Rose-900}"/>
    <SolidColorBrush x:Key="Brush-Rose-950" Color="{StaticResource Color-Rose-950}"/>
    
</ResourceDictionary>