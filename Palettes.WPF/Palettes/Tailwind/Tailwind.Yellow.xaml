<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Color x:Key="Color-Yellow-050">#fefce8</Color>
    <Color x:Key="Color-Yellow-100">#fef9c3</Color>
    <Color x:Key="Color-Yellow-200">#fef08a</Color>
    <Color x:Key="Color-Yellow-300">#fde047</Color>
    <Color x:Key="Color-Yellow-400">#facc15</Color>
    <Color x:Key="Color-Yellow-500">#eab308</Color>
    <Color x:Key="Color-Yellow-600">#ca8a04</Color>
    <Color x:Key="Color-Yellow-700">#a16207</Color>
    <Color x:Key="Color-Yellow-800">#854d0e</Color>
    <Color x:Key="Color-Yellow-900">#713f12</Color>
    <Color x:Key="Color-Yellow-950">#422006</Color>
    
    <SolidColorBrush x:Key="Brush-Yellow-050" Color="{StaticResource Color-Yellow-050}"/>
    <SolidColorBrush x:Key="Brush-Yellow-100" Color="{StaticResource Color-Yellow-100}"/>
    <SolidColorBrush x:Key="Brush-Yellow-200" Color="{StaticResource Color-Yellow-200}"/>
    <SolidColorBrush x:Key="Brush-Yellow-300" Color="{StaticResource Color-Yellow-300}"/>
    <SolidColorBrush x:Key="Brush-Yellow-400" Color="{StaticResource Color-Yellow-400}"/>
    <SolidColorBrush x:Key="Brush-Yellow-500" Color="{StaticResource Color-Yellow-500}"/>
    <SolidColorBrush x:Key="Brush-Yellow-600" Color="{StaticResource Color-Yellow-600}"/>
    <SolidColorBrush x:Key="Brush-Yellow-700" Color="{StaticResource Color-Yellow-700}"/>
    <SolidColorBrush x:Key="Brush-Yellow-800" Color="{StaticResource Color-Yellow-800}"/>
    <SolidColorBrush x:Key="Brush-Yellow-900" Color="{StaticResource Color-Yellow-900}"/>
    <SolidColorBrush x:Key="Brush-Yellow-950" Color="{StaticResource Color-Yellow-950}"/>
    
</ResourceDictionary>