<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Color x:Key="Color-Sky-050">#f0f9ff</Color>
    <Color x:Key="Color-Sky-100">#e0f2fe</Color>
    <Color x:Key="Color-Sky-200">#bae6fd</Color>
    <Color x:Key="Color-Sky-300">#7dd3fc</Color>
    <Color x:Key="Color-Sky-400">#38bdf8</Color>
    <Color x:Key="Color-Sky-500">#0ea5e9</Color>
    <Color x:Key="Color-Sky-600">#0284c7</Color>
    <Color x:Key="Color-Sky-700">#0369a1</Color>
    <Color x:Key="Color-Sky-800">#075985</Color>
    <Color x:Key="Color-Sky-900">#0c4a6e</Color>
    <Color x:Key="Color-Sky-950">#082f49</Color>
    
    <SolidColorBrush x:Key="Brush-Sky-050" Color="{StaticResource Color-Sky-050}"/>
    <SolidColorBrush x:Key="Brush-Sky-100" Color="{StaticResource Color-Sky-100}"/>
    <SolidColorBrush x:Key="Brush-Sky-200" Color="{StaticResource Color-Sky-200}"/>
    <SolidColorBrush x:Key="Brush-Sky-300" Color="{StaticResource Color-Sky-300}"/>
    <SolidColorBrush x:Key="Brush-Sky-400" Color="{StaticResource Color-Sky-400}"/>
    <SolidColorBrush x:Key="Brush-Sky-500" Color="{StaticResource Color-Sky-500}"/>
    <SolidColorBrush x:Key="Brush-Sky-600" Color="{StaticResource Color-Sky-600}"/>
    <SolidColorBrush x:Key="Brush-Sky-700" Color="{StaticResource Color-Sky-700}"/>
    <SolidColorBrush x:Key="Brush-Sky-800" Color="{StaticResource Color-Sky-800}"/>
    <SolidColorBrush x:Key="Brush-Sky-900" Color="{StaticResource Color-Sky-900}"/>
    <SolidColorBrush x:Key="Brush-Sky-950" Color="{StaticResource Color-Sky-950}"/>
    
</ResourceDictionary>