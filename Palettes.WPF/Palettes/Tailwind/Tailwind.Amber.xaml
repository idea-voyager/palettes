<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Color x:Key="Color-Amber-050">#fffbeb</Color>
    <Color x:Key="Color-Amber-100">#fef3c7</Color>
    <Color x:Key="Color-Amber-200">#fde68a</Color>
    <Color x:Key="Color-Amber-300">#fcd34d</Color>
    <Color x:Key="Color-Amber-400">#fbbf24</Color>
    <Color x:Key="Color-Amber-500">#f59e0b</Color>
    <Color x:Key="Color-Amber-600">#d97706</Color>
    <Color x:Key="Color-Amber-700">#b45309</Color>
    <Color x:Key="Color-Amber-800">#92400e</Color>
    <Color x:Key="Color-Amber-900">#78350f</Color>
    <Color x:Key="Color-Amber-950">#451a03</Color>
    
    <SolidColorBrush x:Key="Brush-Amber-050" Color="{StaticResource Color-Amber-050}"/>
    <SolidColorBrush x:Key="Brush-Amber-100" Color="{StaticResource Color-Amber-100}"/>
    <SolidColorBrush x:Key="Brush-Amber-200" Color="{StaticResource Color-Amber-200}"/>
    <SolidColorBrush x:Key="Brush-Amber-300" Color="{StaticResource Color-Amber-300}"/>
    <SolidColorBrush x:Key="Brush-Amber-400" Color="{StaticResource Color-Amber-400}"/>
    <SolidColorBrush x:Key="Brush-Amber-500" Color="{StaticResource Color-Amber-500}"/>
    <SolidColorBrush x:Key="Brush-Amber-600" Color="{StaticResource Color-Amber-600}"/>
    <SolidColorBrush x:Key="Brush-Amber-700" Color="{StaticResource Color-Amber-700}"/>
    <SolidColorBrush x:Key="Brush-Amber-800" Color="{StaticResource Color-Amber-800}"/>
    <SolidColorBrush x:Key="Brush-Amber-900" Color="{StaticResource Color-Amber-900}"/>
    <SolidColorBrush x:Key="Brush-Amber-950" Color="{StaticResource Color-Amber-950}"/>

</ResourceDictionary>