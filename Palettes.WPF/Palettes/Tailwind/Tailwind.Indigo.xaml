<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Color x:Key="Color-Indigo-050">#eef2ff</Color>
    <Color x:Key="Color-Indigo-100">#e0e7ff</Color>
    <Color x:Key="Color-Indigo-200">#c7d2fe</Color>
    <Color x:Key="Color-Indigo-300">#a5b4fc</Color>
    <Color x:Key="Color-Indigo-400">#818cf8</Color>
    <Color x:Key="Color-Indigo-500">#6366f1</Color>
    <Color x:Key="Color-Indigo-600">#4f46e5</Color>
    <Color x:Key="Color-Indigo-700">#4338ca</Color>
    <Color x:Key="Color-Indigo-800">#3730a3</Color>
    <Color x:Key="Color-Indigo-900">#312e81</Color>
    <Color x:Key="Color-Indigo-950">#1e1b4b</Color>
    
    <SolidColorBrush x:Key="Brush-Indigo-050" Color="{StaticResource Color-Indigo-050}"/>
    <SolidColorBrush x:Key="Brush-Indigo-100" Color="{StaticResource Color-Indigo-100}"/>
    <SolidColorBrush x:Key="Brush-Indigo-200" Color="{StaticResource Color-Indigo-200}"/>
    <SolidColorBrush x:Key="Brush-Indigo-300" Color="{StaticResource Color-Indigo-300}"/>
    <SolidColorBrush x:Key="Brush-Indigo-400" Color="{StaticResource Color-Indigo-400}"/>
    <SolidColorBrush x:Key="Brush-Indigo-500" Color="{StaticResource Color-Indigo-500}"/>
    <SolidColorBrush x:Key="Brush-Indigo-600" Color="{StaticResource Color-Indigo-600}"/>
    <SolidColorBrush x:Key="Brush-Indigo-700" Color="{StaticResource Color-Indigo-700}"/>
    <SolidColorBrush x:Key="Brush-Indigo-800" Color="{StaticResource Color-Indigo-800}"/>
    <SolidColorBrush x:Key="Brush-Indigo-900" Color="{StaticResource Color-Indigo-900}"/>
    <SolidColorBrush x:Key="Brush-Indigo-950" Color="{StaticResource Color-Indigo-950}"/>
    
</ResourceDictionary>