<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Color x:Key="Color-BlueGray-050"> #ECEFF1</Color>
    <Color x:Key="Color-BlueGray-100"> #CFD8DC</Color>
    <Color x:Key="Color-BlueGray-200"> #B0BEC5</Color>
    <Color x:Key="Color-BlueGray-300"> #90A4AE</Color>
    <Color x:Key="Color-BlueGray-400"> #78909C</Color>
    <Color x:Key="Color-BlueGray-500"> #607D8B</Color>
    <Color x:Key="Color-BlueGray-600"> #546E7A</Color>
    <Color x:Key="Color-BlueGray-700"> #455A64</Color>
    <Color x:Key="Color-BlueGray-800"> #37474F</Color>
    <Color x:Key="Color-BlueGray-900"> #263238</Color>
    
    <SolidColorBrush x:Key="Brush-BlueGray-050"  Color="{StaticResource Color-BlueGray-050}"/>
    <SolidColorBrush x:Key="Brush-BlueGray-100"  Color="{StaticResource Color-BlueGray-100}"/>
    <SolidColorBrush x:Key="Brush-BlueGray-200"  Color="{StaticResource Color-BlueGray-200}"/>
    <SolidColorBrush x:Key="Brush-BlueGray-300"  Color="{StaticResource Color-BlueGray-300}"/>
    <SolidColorBrush x:Key="Brush-BlueGray-400"  Color="{StaticResource Color-BlueGray-400}"/>
    <SolidColorBrush x:Key="Brush-BlueGray-500"  Color="{StaticResource Color-BlueGray-500}"/>
    <SolidColorBrush x:Key="Brush-BlueGray-600"  Color="{StaticResource Color-BlueGray-600}"/>
    <SolidColorBrush x:Key="Brush-BlueGray-700"  Color="{StaticResource Color-BlueGray-700}"/>
    <SolidColorBrush x:Key="Brush-BlueGray-800"  Color="{StaticResource Color-BlueGray-800}"/>
    <SolidColorBrush x:Key="Brush-BlueGray-900"  Color="{StaticResource Color-BlueGray-900}"/>
    
</ResourceDictionary>