<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Color x:Key="Color-Yellow-050"> #FFFDE7</Color>
    <Color x:Key="Color-Yellow-100"> #FFF9C4</Color>
    <Color x:Key="Color-Yellow-200"> #FFF59D</Color>
