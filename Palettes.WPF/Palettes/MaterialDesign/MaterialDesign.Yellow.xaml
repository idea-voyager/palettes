    <SolidColorBrush x:Key="Brush-Yellow-700"  Color="{StaticResource Color-Yellow-700}"/>
    <SolidColorBrush x:Key="Brush-Yellow-800"  Color="{StaticResource Color-Yellow-800}"/>
    <SolidColorBrush x:Key="Brush-Yellow-900"  Color="{StaticResource Color-Yellow-900}"/>
    <SolidColorBrush x:Key="Brush-Yellow-A100" Color="{StaticResource Color-Yellow-A100}"/>
    <SolidColorBrush x:Key="Brush-Yellow-A200" Color="{StaticResource Color-Yellow-A200}"/>
    <SolidColorBrush x:Key="Brush-Yellow-A400" Color="{StaticResource Color-Yellow-A400}"/>
