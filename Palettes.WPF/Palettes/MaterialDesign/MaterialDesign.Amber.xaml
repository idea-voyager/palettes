<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Color x:Key="Color-Amber-050"> #FFF8E1</Color>
    <Color x:Key="Color-Amber-100"> #FFECB3</Color>
    <Color x:Key="Color-Amber-200"> #FFE082</Color>
    <Color x:Key="Color-Amber-300"> #FFD54F</Color>
    <Color x:Key="Color-Amber-400"> #FFCA28</Color>
    <Color x:Key="Color-Amber-500"> #FFC107</Color>
    <Color x:Key="Color-Amber-600"> #FFB300</Color>
    <Color x:Key="Color-Amber-700"> #FFA000</Color>
    <Color x:Key="Color-Amber-800"> #FF8F00</Color>
    <Color x:Key="Color-Amber-900"> #FF6F00</Color>
    <Color x:Key="Color-Amber-A100">#FFE57F</Color>
    <Color x:Key="Color-Amber-A200">#FFD740</Color>
    <Color x:Key="Color-Amber-A400">#FFC400</Color>
    <Color x:Key="Color-Amber-A700">#FFAB00</Color>
    
    <SolidColorBrush x:Key="Brush-Amber-050"  Color="{StaticResource Color-Amber-050}"/>
    <SolidColorBrush x:Key="Brush-Amber-100"  Color="{StaticResource Color-Amber-100}"/>
    <SolidColorBrush x:Key="Brush-Amber-200"  Color="{StaticResource Color-Amber-200}"/>
    <SolidColorBrush x:Key="Brush-Amber-300"  Color="{StaticResource Color-Amber-300}"/>
    <SolidColorBrush x:Key="Brush-Amber-400"  Color="{StaticResource Color-Amber-400}"/>
    <SolidColorBrush x:Key="Brush-Amber-500"  Color="{StaticResource Color-Amber-500}"/>
    <SolidColorBrush x:Key="Brush-Amber-600"  Color="{StaticResource Color-Amber-600}"/>
    <SolidColorBrush x:Key="Brush-Amber-700"  Color="{StaticResource Color-Amber-700}"/>
    <SolidColorBrush x:Key="Brush-Amber-800"  Color="{StaticResource Color-Amber-800}"/>
    <SolidColorBrush x:Key="Brush-Amber-900"  Color="{StaticResource Color-Amber-900}"/>
    <SolidColorBrush x:Key="Brush-Amber-A100" Color="{StaticResource Color-Amber-A100}"/>
    <SolidColorBrush x:Key="Brush-Amber-A200" Color="{StaticResource Color-Amber-A200}"/>
    <SolidColorBrush x:Key="Brush-Amber-A400" Color="{StaticResource Color-Amber-A400}"/>
    <SolidColorBrush x:Key="Brush-Amber-A700" Color="{StaticResource Color-Amber-A700}"/>

</ResourceDictionary>