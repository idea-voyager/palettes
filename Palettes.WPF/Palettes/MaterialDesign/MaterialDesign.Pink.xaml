<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Color x:Key="Color-Pink-050"> #FCE4EC</Color>
    <Color x:Key="Color-Pink-100"> #F8BBD0</Color>
    <Color x:Key="Color-Pink-200"> #F48FB1</Color>
    <Color x:Key="Color-Pink-300"> #F06292</Color>
    <Color x:Key="Color-Pink-400"> #EC407A</Color>
    <Color x:Key="Color-Pink-500"> #E91E63</Color>
    <Color x:Key="Color-Pink-600"> #D81B60</Color>
    <Color x:Key="Color-Pink-700"> #C2185B</Color>
    <Color x:Key="Color-Pink-800"> #AD1457</Color>
    <Color x:Key="Color-Pink-900"> #880E4F</Color>
    <Color x:Key="Color-Pink-A100">#FF80AB</Color>
    <Color x:Key="Color-Pink-A200">#FF4081</Color>
    <Color x:Key="Color-Pink-A400">#F50057</Color>
    <Color x:Key="Color-Pink-A700">#C51162</Color>
    
    <SolidColorBrush x:Key="Brush-Pink-050"  Color="{StaticResource Color-Pink-050}"/>
    <SolidColorBrush x:Key="Brush-Pink-100"  Color="{StaticResource Color-Pink-100}"/>
    <SolidColorBrush x:Key="Brush-Pink-200"  Color="{StaticResource Color-Pink-200}"/>
    <SolidColorBrush x:Key="Brush-Pink-300"  Color="{StaticResource Color-Pink-300}"/>
    <SolidColorBrush x:Key="Brush-Pink-400"  Color="{StaticResource Color-Pink-400}"/>
    <SolidColorBrush x:Key="Brush-Pink-500"  Color="{StaticResource Color-Pink-500}"/>
    <SolidColorBrush x:Key="Brush-Pink-600"  Color="{StaticResource Color-Pink-600}"/>
    <SolidColorBrush x:Key="Brush-Pink-700"  Color="{StaticResource Color-Pink-700}"/>
    <SolidColorBrush x:Key="Brush-Pink-800"  Color="{StaticResource Color-Pink-800}"/>
    <SolidColorBrush x:Key="Brush-Pink-900"  Color="{StaticResource Color-Pink-900}"/>
    <SolidColorBrush x:Key="Brush-Pink-A100" Color="{StaticResource Color-Pink-A100}"/>
    <SolidColorBrush x:Key="Brush-Pink-A200" Color="{StaticResource Color-Pink-A200}"/>
    <SolidColorBrush x:Key="Brush-Pink-A400" Color="{StaticResource Color-Pink-A400}"/>
    <SolidColorBrush x:Key="Brush-Pink-A700" Color="{StaticResource Color-Pink-A700}"/>
    
</ResourceDictionary>