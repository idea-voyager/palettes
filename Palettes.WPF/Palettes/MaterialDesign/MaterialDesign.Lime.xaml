<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Color x:Key="Color-Lime-050"> #F9FBE7</Color>
    <Color x:Key="Color-Lime-100"> #F0F4C3</Color>
    <Color x:Key="Color-Lime-200"> #E6EE9C</Color>
    <Color x:Key="Color-Lime-300"> #DCE775</Color>
    <Color x:Key="Color-Lime-400"> #D4E157</Color>
    <Color x:Key="Color-Lime-500"> #CDDC39</Color>
    <Color x:Key="Color-Lime-600"> #C0CA33</Color>
    <Color x:Key="Color-Lime-700"> #AFB42B</Color>
    <Color x:Key="Color-Lime-800"> #9E9D24</Color>
    <Color x:Key="Color-Lime-900"> #827717</Color>
    <Color x:Key="Color-Lime-A100">#F4FF81</Color>
    <Color x:Key="Color-Lime-A200">#EEFF41</Color>
    <Color x:Key="Color-Lime-A400">#C6FF00</Color>
    <Color x:Key="Color-Lime-A700">#AEEA00</Color>
    
    <SolidColorBrush x:Key="Brush-Lime-050"  Color="{StaticResource Color-Lime-050}"/>
    <SolidColorBrush x:Key="Brush-Lime-100"  Color="{StaticResource Color-Lime-100}"/>
    <SolidColorBrush x:Key="Brush-Lime-200"  Color="{StaticResource Color-Lime-200}"/>
    <SolidColorBrush x:Key="Brush-Lime-300"  Color="{StaticResource Color-Lime-300}"/>
    <SolidColorBrush x:Key="Brush-Lime-400"  Color="{StaticResource Color-Lime-400}"/>
    <SolidColorBrush x:Key="Brush-Lime-500"  Color="{StaticResource Color-Lime-500}"/>
    <SolidColorBrush x:Key="Brush-Lime-600"  Color="{StaticResource Color-Lime-600}"/>
    <SolidColorBrush x:Key="Brush-Lime-700"  Color="{StaticResource Color-Lime-700}"/>
    <SolidColorBrush x:Key="Brush-Lime-800"  Color="{StaticResource Color-Lime-800}"/>
    <SolidColorBrush x:Key="Brush-Lime-900"  Color="{StaticResource Color-Lime-900}"/>
    <SolidColorBrush x:Key="Brush-Lime-A100" Color="{StaticResource Color-Lime-A100}"/>
    <SolidColorBrush x:Key="Brush-Lime-A200" Color="{StaticResource Color-Lime-A200}"/>
    <SolidColorBrush x:Key="Brush-Lime-A400" Color="{StaticResource Color-Lime-A400}"/>
    <SolidColorBrush x:Key="Brush-Lime-A700" Color="{StaticResource Color-Lime-A700}"/>
    
</ResourceDictionary>