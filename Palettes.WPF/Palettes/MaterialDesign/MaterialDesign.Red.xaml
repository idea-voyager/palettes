<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Color x:Key="Color-Red-050"> #FFEBEE</Color>
    <Color x:Key="Color-Red-100"> #FFCDD2</Color>
    <Color x:Key="Color-Red-200"> #EF9A9A</Color>
    <Color x:Key="Color-Red-300"> #E57373</Color>
    <Color x:Key="Color-Red-400"> #EF5350</Color>
    <Color x:Key="Color-Red-500"> #F44336</Color>
    <Color x:Key="Color-Red-600"> #E53935</Color>
    <Color x:Key="Color-Red-700"> #D32F2F</Color>
    <Color x:Key="Color-Red-800"> #C62828</Color>
    <Color x:Key="Color-Red-900"> #B71C1C</Color>
    <Color x:Key="Color-Red-A100">#FF8A80</Color>
    <Color x:Key="Color-Red-A200">#FF5252</Color>
    <Color x:Key="Color-Red-A400">#FF1744</Color>
    <Color x:Key="Color-Red-A700">#D50000</Color>
    
    <SolidColorBrush x:Key="Brush-Red-050"  Color="{StaticResource Color-Red-050}"/>
    <SolidColorBrush x:Key="Brush-Red-100"  Color="{StaticResource Color-Red-100}"/>
    <SolidColorBrush x:Key="Brush-Red-200"  Color="{StaticResource Color-Red-200}"/>
    <SolidColorBrush x:Key="Brush-Red-300"  Color="{StaticResource Color-Red-300}"/>
    <SolidColorBrush x:Key="Brush-Red-400"  Color="{StaticResource Color-Red-400}"/>
    <SolidColorBrush x:Key="Brush-Red-500"  Color="{StaticResource Color-Red-500}"/>
    <SolidColorBrush x:Key="Brush-Red-600"  Color="{StaticResource Color-Red-600}"/>
    <SolidColorBrush x:Key="Brush-Red-700"  Color="{StaticResource Color-Red-700}"/>
    <SolidColorBrush x:Key="Brush-Red-800"  Color="{StaticResource Color-Red-800}"/>
    <SolidColorBrush x:Key="Brush-Red-900"  Color="{StaticResource Color-Red-900}"/>
    <SolidColorBrush x:Key="Brush-Red-A100" Color="{StaticResource Color-Red-A100}"/>
    <SolidColorBrush x:Key="Brush-Red-A200" Color="{StaticResource Color-Red-A200}"/>
    <SolidColorBrush x:Key="Brush-Red-A400" Color="{StaticResource Color-Red-A400}"/>
    <SolidColorBrush x:Key="Brush-Red-A700" Color="{StaticResource Color-Red-A700}"/>
    
</ResourceDictionary>