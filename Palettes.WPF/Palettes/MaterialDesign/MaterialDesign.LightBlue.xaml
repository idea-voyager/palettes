    <SolidColorBrush x:Key="Brush-LightBlue-900"  Color="{StaticResource Color-LightBlue-900}"/>
    <SolidColorBrush x:Key="Brush-LightBlue-A100" Color="{StaticResource Color-LightBlue-A100}"/>
    <SolidColorBrush x:Key="Brush-LightBlue-A200" Color="{StaticResource Color-LightBlue-A200}"/>
